{
  "schemaVersion": 1,
  "id": "dimdoors",
  "version": "${version}",
  "name": "DimensionalDoors",
  "description": "Bend and twist reality, creating rifts, pocket dimensions, and much more",
  "authors": [
    "StevenRS11",
    "SenseiKiwi",
    "Zixiken",
    "Waterpicker",
    "Robijnvogel",
    "ZombieHDGaming",
    "Runemoro",
    "Mysticpasta1"
  ],
  "contact": {
    "homepage": "https://dimdev.org/",
    "sources": "https://github.com/DimensionalDevelopment/DimDoors",
    "issues": "https://github.com/DimensionalDevelopment/DimDoors/issues"
  },
  "license": "MIT",
  "icon": "assets/dimdoors/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.dimdev.dimdoors.DimensionalDoorsFabric"
    ],
    "client": [
      "org.dimdev.dimdoors.client.DimensionalDoorsClientFabric"
    ],
    "cardinal-components": [
      "org.dimdev.dimdoors.DimensionalDoorsComponents"
    ],
    "fabric-datagen": [
      "org.dimdev.dimdoors.datagen.DatagenInitializer"
    ],
    "modmenu": [ "org.dimdev.dimdoors.compat.ModMenuCompat" ],
    "mm:early_risers": [
      "org.dimdev.dimdoors.EarlyRiser"
    ],
    "rei_client": [
      "org.dimdev.dimdoors.compat.rei.TesselatingReiCompatClient"
    ]
  },
  "mixins": [
    "dimdoors.mixins.json",
    "dimdoors-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}",
    "architectury": ">=${architectury_version}",
    "cloth-config": ">=${cloth_config_version}"
  },
  "custom": {
    "cardinal-components": [
      "dimdoors:dimensional_registry",
      "dimdoors:counter",
      "dimdoors:chunk_lazily_generated",
      "dimdoors:player_modifiers"
    ],
    "loom:injected_interfaces": {
      "net/minecraft/class_2323": ["org/dimdev/dimdoors/block/DoorSoundProvider"],
      "net/minecraft/class_2533": ["org/dimdev/dimdoors/block/DoorSoundProvider"]
    }
  }
}
